{
    "name": "chiral-db-grpc-client-ts",
    "version": "0.2.0",
    "description": "ChiralDB gRPC TypeScript Client",
    "main": "dist/client.js",
    "types": "dist/client.d.ts",
    "scripts": {
      "copy": "cp proto_generated_ts/*.js dist/proto_generated_ts/",
      "build": "npx tsc & npm run copy & npx webpack",
      "test": "npx tsc & npm run copy & npx webpack --config webpack.test.config.js"
    },
    "dependencies": {
      "google-protobuf": "~3.14.0",
      "grpc-web": "~1.3.1"
    },
    "devDependencies": {
      "@types/google-protobuf": "~3.7.0",
      "@types/jquery": "~3.3.6",
      "@types/node": "~10.17.0",
      "jquery": "~3.5.1",
      "mock-xmlhttprequest": "~2.0.0",
      "typescript": "latest",
      "webpack": "~4.43.0",
      "webpack-cli": "~3.3.11"
    }
  }